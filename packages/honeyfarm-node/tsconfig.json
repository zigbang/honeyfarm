{
	"compilerOptions": {
		"baseUrl": "./",
		"outDir": "./dist",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"allowJs": true,
		"esModuleInterop": true,
		"isolatedModules": false,
		"noImplicitAny": false,
		"jsx": "react",
		"lib": ["es2015","es5", "es6","dom", "ES2017"],
		"moduleResolution": "node",
		"module": "commonjs",
		"noEmit": false,
		"strict": false,
		"target": "es5",
    	"resolveJsonModule": true
	},
}
